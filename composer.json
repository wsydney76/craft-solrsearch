{
  "name": "wsydney76/solrsearch",
  "description": "Integrates Solr search engine",
  "type": "craft-plugin",
  "version": "4.0.4",
  "keywords": [
    "craft",
    "cms",
    "craftcms",
    "craft-plugin",
    "solrsearch"
  ],
  "support": {
    "docs": "https://github.com/wsydney76/solrsearch/blob/master/README.md",
    "issues": "https://github.com/wsydney76/solrsearch/issues"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "wsydney76",
      "homepage": "https://github.com/wsydney76"
    }
  ],
  "require": {
    "craftcms/cms": "^4.0.0"
  },
  "autoload": {
    "psr-4": {
      "wsydney76\\solrsearch\\": "src/"
    }
  },
  "extra": {
    "name": "Solr Search",
    "handle": "solrsearch",
    "hasCpSettings": true,
    "hasCpSection": false,
    "changelogUrl": "https://raw.githubusercontent.com/wsydney76/solrsearch/master/CHANGELOG.md",
    "class": "wsydney76\\solrsearch\\SolrSearch"
  }
}
